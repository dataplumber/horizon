distribute.mrf.output.path=/tmp/TIE_TEST/mrfoutput
distribute.sleep.seconds=60

# Cache path defaults if left empty to {user.home}/.horizon/subscriber-cache
distribute.cache.path=

distribute.data.retriever.class=gov.nasa.gibs.distribute.subscriber.plugins.BasicDataRetriever
distribute.subscriber.class=gov.nasa.gibs.distribute.subscriber.plugins.IWSSubscriber
distribute.granule.file.size=100

sigevent.url=http://localhost:8100/sigevent
inventory.url=http://localhost:9192
zookeeper.url=localhost:2181

distribute.output.base.path=${distribute.output.base.path}
distribute.exec.cmd=sh ${distribute.home}/bin/ascatInfo.sh  $gName $dsName ${distribute.home}/subscriber $gDir

execution.sig.category=SUBSCRIBER
distribute.data.location=${distribute.data.location}
